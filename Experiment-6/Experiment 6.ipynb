   "source": [
    "import cv2\n",
    "img=cv2.imread('img.jpg')\n",
    "mask=cv2.imread('massk.png',0)\n",
    "dst=cv2.inpaint(img,mask, 50, cv2. INPAINT_NS)\n",
    "cv2.imwrite('resorted.png',dst)\n",
    "cv2.imshow('Restorted Image',dst)\n",
    "cv2.waitKey(0)"
